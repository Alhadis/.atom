"*":
  "atom-material-ui":
    fonts: {}
    tabs: {}
    ui: {}
  "auto-detect-indentation":
    showSpacingInStatusBar: false
  "autoclose-html": {}
  "autocomplete-plus": {}
  core:
    customFileTypes:
      "source.turing": [
        "t"
      ]
      "text.roff": [
        "mm"
      ]
    disabledPackages: [
      "spell-check"
      "autocomplete-css"
      "background-tips"
      "welcome"
      "language-java"
      "language-python"
      "autocomplete-plus"
    ]
    openEmptyEditorOnStart: false
    themes: [
      "atom-light-ui"
      "Phoenix-Syntax"
    ]
  docblockr: {}
  editor:
    autoIndentOnPaste: false
    fontFamily: "Menlig"
    fontSize: 11
    invisibles: {}
    lineHeight: 1.3
    softTabs: false
    tabLength: 4
  "exception-reporting":
    userId: "4b2dfe3a-7eef-a871-4d35-230cb11366f8"
  "file-icons":
    coloured: false
    debugging: {}
    iconMatching: {}
  "find-and-replace":
    showSearchWrapIcon: false
  "grammar-selector": {}
  "language-applescript":
    autoCompile: false
  "lisp-paredit": {}
  "markdown-preview":
    useGitHubStyle: true
  minimap:
    autoToggle: false
  popupDismissDelay: false
  "recent-files":
    updated: false
  "seti-ui":
    compactView: true
    fileIcons: false
  "spell-check": {}
  "status-bar":
    fullWidth: false
  "symbols-tree-view":
    scrollAnimation: false
  tabs: {}
  "toggle-quotes": {}
  "tree-view":
    hideIgnoredNames: true
  welcome:
    showOnStartup: false
  whitespace:
    removeTrailingWhitespace: false
".apl.source":
  editor:
    fontSize: 24
".atom-mocha":
  editor:
    fontSize: 13
