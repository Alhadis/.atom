[
	{
		title: ".atom"
		paths: [
			"~/.atom"
		]
		icon: "atom-icon"
	}
	{
		title: ".files"
		paths: [
			"~/.files"
		]
		icon: "terminal-icon"
	}
	{
		title: "Utils"
		paths: [
			"~/Labs/Utils"
			"~/Labs/JG"
			"~/Labs/EAL"
		]
		icon: "icon-tools"
	}
	{
		title: "language-roff"
		paths: [
			"~/Labs/language-roff"
			"~/Labs/Roff.js"
		]
		icon: "manpage-icon"
	}
	{
		title: "Grammars, etc"
		paths: [
			"~/Labs/language-etc"
			"~/.atom/packages/language-grammars"
			"~/Labs/language-regexp"
			"~/.atom/packages/language-fontforge"
			"~/.atom/packages/language-viml"
		]
		icon: "icon-github"
	}
	{
		title: "file-icons"
		paths: [
			"~/Labs/file-icons/atom"
			"~/Labs/file-icons/compiler"
			"~/Labs/file-icons/icons"
			"~/Labs/file-icons/vscode"
			"~/Labs/file-icons/atom-fs"
		]
		icon: "atom-icon"
	}
	{
		title: "Mono"
		paths: [
			"~/Labs/Mono"
			"~/Labs/V8.man"
			"~/Forks/GNU-Groff"
			"/usr/local/share/groff"
		]
		icon: "manpage-icon"
	}
	{
		title: "YASR"
		paths: [
			"~/Labs/YASR"
		]
		icon: "emacs-icon"
	}
	{
		title: "Homebrew Troff"
		paths: [
			"~/Labs/homebrew-troff"
			"~/Forks/heirloom-doctools"
			"~/Forks/man-db"
			"/usr/local/Homebrew/Library/Taps/homebrew/homebrew-core"
		]
		icon: "icon-ruby"
		color: "#ff0000"
	}
	{
		title: "atom-live-view"
		paths: [
			"~/Labs/Atom-LiveView"
			"~/Labs/Atom-PostScriptPreview"
		]
		icon: "icon-atom"
	}
	{
		title: "Postage"
		paths: [
			"~/Labs/Atom-PostScript"
			"~/Forks/GhostScript"
			"~/Forks/postscriptbarcode"
		]
		icon: "postscript-icon"
		color: "#da291c"
	}
]
