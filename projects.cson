[
	{
		title: ".{files,atom,emacs.d}"
		paths: [
			"~/.files"
			"~/.atom"
			"~/.emacs.d"
			"~/Labs/YASR"
		]
		icon: "terminal-icon"
	}
	{
		title: "Utils"
		paths: [
			"~/Labs/Utils"
			"~/Labs/JG"
			"~/Labs/EAL"
		]
		icon: "icon-tools"
	}
	{
		title: "language-roff"
		paths: [
			"~/Labs/language-roff"
			"~/Labs/Roff.js"
		]
		icon: "manpage-icon"
	}
	{
		title: "Grammars, etc"
		paths: [
			"~/Labs/language-etc"
			"~/.atom/packages/language-grammars"
			"~/Labs/language-regexp"
			"~/Labs/language-roff"
			"~/.atom/packages/language-fontforge"
			"~/.atom/packages/language-viml"
			"~/.atom/packages/language-webassembly"
		]
		icon: "bnf-icon"
	}
	{
		title: "file-icons"
		paths: [
			"~/Labs/file-icons/atom"
			"~/Labs/file-icons/compiler"
			"~/Labs/file-icons/icons"
			"~/Labs/file-icons/vscode"
			"~/Labs/file-icons/atom-fs"
		]
		icon: "atom-icon"
		color: "#40a977"
	}
	{
		title: "Mono"
		paths: [
			"~/Labs/Mono"
			"~/Labs/V8.man"
			"~/Forks/GNU-Groff"
			"/usr/local/share/groff"
			"~/Forks/DWB3.3"
		]
		icon: "manpage-icon"
	}
	{
		title: "Homebrew Troff"
		paths: [
			"~/Labs/homebrew-troff"
			"/usr/local/Homebrew"
			"~/Forks/heirloom-doctools"
			"~/Forks/neatroff_make"
		]
		icon: "icon-ruby"
		color: "#ff0000"
	}
	{
		title: "atom-live-view"
		paths: [
			"~/Labs/Atom-LiveView"
			"~/Labs/Atom-PostScriptPreview"
		]
		icon: "atom-icon"
		color: "#40a977"
	}
	{
		title: "Postage"
		paths: [
			"~/Labs/Stupid-Post-Tricks"
			"~/Labs/Inspect.ps"
			"~/Labs/Atom-PostScript"
			"~/Forks/GhostScript"
			"~/Forks/postscriptbarcode"
			"/usr/local/share/ghostscript/9.53.3/Resource/Init"
		]
		icon: "postscript-icon"
		color: "#da291c"
	}
	{
		title: "Vim / Vi"
		paths: [
			"~/Forks/nvi"
			"~/Forks/OpenBSD/usr.bin/vi"
			"~/Forks/Vim"
		]
		icon: "vim-icon"
		color: "#019833"
	}
	{
		title: "Atom CI"
		paths: [
			"~/Labs/Atom-CI"
			"~/Labs/Atom-Mocha"
		]
		icon: "atom-icon"
		color: "#40a977"
	}
]
